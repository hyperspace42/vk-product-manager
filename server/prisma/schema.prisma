generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  date        DateTime
  username    String
  productName String
  body        String
  chatUrl     String
  completed   Boolean
}
